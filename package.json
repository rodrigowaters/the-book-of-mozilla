{
  "name": "the-book-of-mozilla",
  "description": "O Livro de Mozilla",
  "version": "2.0.0",
  "author": "Rodrigo Silva",
  "homepage": "http://rodrigowaters.github.io/the-book-of-mozilla/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "app:emulate": "ionic cordova emulate",
    "app:run": "ionic cordova run",
    "app:build": "ionic cordova build --release",
    "app:sign": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore the-book-of-mozilla.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk the_book_of_mozilla",
    "app:create": "zipalign -v 4 platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk the-book-of-mozilla.apk",
    "deploy": "ionic build --prod -- --base-href http://rodrigowaters.github.io/the-book-of-mozilla/",
    "deploy_gh": "npx angular-cli-ghpages --dir=www --name='Rodrigo Silva' --email=rdrgwtrs@gmail.com",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@ionic-native/core": "5.26.0",
    "@ionic-native/splash-screen": "5.26.0",
    "@ionic-native/status-bar": "5.26.0",
    "@ionic/angular": "5.1.1",
    "angular-cli-ghpages": "0.6.2",
    "cordova-android": "8.1.0",
    "core-js": "3.6.5",
    "rxjs": "6.5.5",
    "tslib": "2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.901.7",
    "@angular-devkit/build-angular": "0.803.26",
    "@angular-devkit/core": "9.1.7",
    "@angular-devkit/schematics": "9.1.7",
    "@angular/cli": "8.3.25",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "9.1.11",
    "@ionic/angular-toolkit": "2.2.0",
    "@types/jasmine": "3.5.10",
